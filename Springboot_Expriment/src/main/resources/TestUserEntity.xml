<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.springboot_test_quick.User">
        <table name="test_user" schema="testhibernate" catalog=""/>
